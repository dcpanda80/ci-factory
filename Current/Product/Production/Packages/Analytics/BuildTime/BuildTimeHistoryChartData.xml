  <chart_data>
    <row>
      <null/>
      &XAxis;
    </row>
    <row>
      <string>Total</string>
      &Total;
    </row>
    <row>
      <string>Compile</string>
      &Compile;
    </row>
    <row>
      <string>Product Source Update</string>
      &ProductSourceUpdate;
    </row>
    <row>
      <string>Third Party Source Update</string>
      &ThirdPartySourceUpdate;
    </row>
    <row>

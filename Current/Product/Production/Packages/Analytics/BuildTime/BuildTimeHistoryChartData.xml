  <chart_type>
    <string>line</string>
    <string>line</string>
    <string>area</string>
    <string>area</string>
    <string>area</string>
  </chart_type>
  <axis_category skip='50' />

  <draw>
    <text color='000000' rotation='-90' size='35' x='5' y='275' width='200' height='80' h_align='center'>Seconds</text>
  </draw>

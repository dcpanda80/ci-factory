<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE chart [
  <!ENTITY % entities SYSTEM "entities.dtd">

  %entities;

  
  <!ENTITY XAxis                    SYSTEM    "BuildHistoryXAxis.xml">

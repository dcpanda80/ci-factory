<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE chart [
  <!ENTITY XAxis SYSTEM     "..\..\..\Installs\RecoveryTimeHistoryXAxis.xml">
  <!ENTITY RecoveryTimeMax SYSTEM   "..\..\..\Installs\RecoveryTimeMaxHistoryDurations.xml">
  <!ENTITY RecoveryTimeMin SYSTEM   "..\..\..\Installs\RecoveryTimeMinHistoryDurations.xml">
  <!ENTITY RecoveryTimeOpen SYSTEM   "..\..\..\Installs\RecoveryTimeOpenHistoryDurations.xml">
  <!ENTITY RecoveryTimeClose SYSTEM   "..\..\..\Installs\RecoveryTimeCloseHistoryDurations.xml">
]>
<chart>

  <chart_data>
    <row>
      &XAxis;
    </row>
    <row>
      &RecoveryTimeMax;
    </row>
    <row>
      &RecoveryTimeMin;
    </row>
    <row>
      &RecoveryTimeOpen;
    </row>
    <row>
      &RecoveryTimeClose;
    </row>
  </chart_data>

  <chart_type>candlestick</chart_type>

  <draw>
    <text color='000000' size='35' x='200' y='25' width='275' height='80' h_align='center'>Recovery Times</text>
    <text color='000000' rotation='-90' size='35' x='5' y='275' width='200' height='80' h_align='center'>minutes</text>
  </draw>
</chart>
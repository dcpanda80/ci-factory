<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE chart [
  <!ENTITY % entities SYSTEM "entities.dtd">

  %entities;

  
  <!ENTITY XAxis      SYSTEM  "SuccessProgressXAxis.xml">
  <!ENTITY Success    SYSTEM  "SuccessProgressSuccessful.xml">
  <!ENTITY Fail       SYSTEM  "SuccessProgressFailed.xml">
  <!ENTITY Exception  SYSTEM  "SuccessProgressException.xml">
]>
<chart>

  <chart_data>
    <row>
      <null/>
      &XAxis;
    </row>
    <row>
      <string>Success</string>
      &Success;
      </row>
    <row>
      <string>Fail</string>
      &Fail;
      </row>
    <row>
      <string>Exception</string>
      &Exception;
      </row>
  </chart_data>

  <chart_type>line</chart_type>
  <axis_category skip='100' />
  <series_color>
    <color>41B32D</color>
    <color>D13535</color>
    <color>000000</color>
  </series_color>

  <draw>
    <text color='000000' rotation='-90' size='35' x='5' y='275' width='200' height='80' h_align='center'>Count</text>
  </draw>
</chart>
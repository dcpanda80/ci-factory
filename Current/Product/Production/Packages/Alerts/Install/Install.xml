<?xml version="1.0" encoding="utf-8" ?>
<project xmlns="http://nant.sf.net/schemas/nant.xsd" name="Alertss.Install" default="Alertss.Install">

  <include buildfile="Properties.xml" />
  
  <target name="Alertss.Install">
    <largeproperty name="Packages.AddPostBuildActions.Block">
      <value xml="false">
      <![CDATA[<call target="Alertss.Run" />]]></value>
    </largeproperty>
    <call target="Packages.AddPostBuildActions"/>
  </target>

  <target name="Alertss.SetAsCurrentPackage">
    <property name="Packages.CurrenctPackage.PackageDirectoryName" value="Alertss"/>
    <property name="Packages.CurrenctPackage.PackageTargetFileName" value="Alertss.Target.xml"/>
    <property name="Packages.CurrenctPackage.PackageName" value="Alertss"/>
    <property name="Current.BuildFile" value="${BuildDirectory}\Post.Build.xml"/>
  </target>

  <target name="Alertss.ValidateParameters">
    
  </target>
  
</project>
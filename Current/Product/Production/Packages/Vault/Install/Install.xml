<?xml version="1.0" encoding="utf-8" ?>
<project xmlns="http://nant.sf.net/schemas/nant.xsd" name="Vault.Installer" default="Install">

  <include buildfile="Properties.xml" />

  <target name="Vault.Install">
    <largeproperty name="Packages.AddCleanUpActions.Block">
      <value xml="false"><![CDATA[<call target="SourceControl.CleanGetOfThirdPartyDirectory" />
      <call target="SourceControl.CleanGetOfProductDirectory" />]]></value>
    </largeproperty>
    <call target="Packages.AddCleanUpActions"/>
  </target>

  <target name="Vault.SetAsCurrentPackage">
    <property name="Packages.CurrentPackage.PackageName" value="Vault"/>
    <property name="Packages.CurrentPackage.PackageType" value="SourceControl"/>
  </target>

  <target name="Vault.ValidateParameters">

  </target>
  
</project>

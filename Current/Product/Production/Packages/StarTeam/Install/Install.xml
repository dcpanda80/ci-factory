<?xml version="1.0" encoding="utf-8" ?>
<project xmlns="http://nant.sf.net/schemas/nant.xsd" name="StarTeam.Installer" default="Install">

  <include buildfile="Properties.xml" />

  <target name="StarTeam.Install">
    <largeproperty name="Packages.AddCleanUpActions.Block">
      <value xml="false"><![CDATA[<call target="SourceControl.CleanGetOf.Common.Directory.ThirdParty.Path" />
      <call target="SourceControl.CleanGetOf.Common.Directory.Product.Path" />]]></value>
    </largeproperty>
    <call target="Packages.AddCleanUpActions"/>
  </target>

  <target name="StarTeam.SetAsCurrentPackage">
    <property name="Packages.CurrentPackage.PackageName" value="StarTeam"/>
    <property name="Packages.CurrentPackage.PackageType" value="SourceControl"/>
  </target>

  <target name="StarTeam.ValidateParameters">

  </target>
  
</project>

<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE chart [
  <!ENTITY XAxis SYSTEM     "..\..\..\Installs\QuietTimeHistoryXAxis.xml">
  <!ENTITY QuiteTimeMax SYSTEM   "..\..\..\Installs\QuietTimeMaxHistoryDurations.xml">
  <!ENTITY QuiteTimeMin SYSTEM   "..\..\..\Installs\QuietTimeMinHistoryDurations.xml">
  <!ENTITY QuiteTimeOpen SYSTEM   "..\..\..\Installs\QuietTimeOpenHistoryDurations.xml">
  <!ENTITY QuiteTimeClose SYSTEM   "..\..\..\Installs\QuietTimeCloseHistoryDurations.xml">
]>
<chart>

  <chart_data>
    <row>
      &XAxis;
    </row>
    <row>
      &QuiteTimeMax;
    </row>
    <row>
      &QuiteTimeMin;
    </row>
    <row>
      &QuiteTimeOpen;
    </row>
    <row>
      &QuiteTimeClose;
    </row>
  </chart_data>

  <chart_type>candlestick</chart_type>

  <draw>
    <text color='000000' size='35' x='200' y='25' width='275' height='80' h_align='center'>Quiet Times</text>
    <text color='000000' rotation='-90' size='35' x='5' y='275' width='200' height='80' h_align='center'>minutes</text>
  </draw>
</chart>